    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^27.4.0",
    "postcss": "^8.4.32",
    "prettier": "^3.7.4",
    "tailwindcss": "^3.4.0",
    "vite": "^5.0.0",
    "vitest": "^4.0.17"
